<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    
    <route id="tadcka_tree_item_root" pattern="/tree-item/item">
        <default key="_controller">TadckaTreeBundle:TreeJson:root</default>
    </route>

    <route id="tadcka_tree_item" pattern="/tree-item/root">
        <default key="_controller">TadckaTreeBundle:TreeItem:item</default>
    </route>

</routes>
