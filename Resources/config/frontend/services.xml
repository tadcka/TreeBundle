<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="tadcka_tree.frontend.helper.frontend.class">Tadcka\Bundle\TreeBundle\Frontend\Helper\FrontendHelper</parameter>
    </parameters>

    <services>
        <!--Frontend helper-->
        <service id="tadcka_tree.frontend.helper.frontend" class="%tadcka_tree.frontend.helper.frontend.class%">
            <argument type="service" id="translator" />
        </service>
    </services>
</container>
